<div class="container">
  <div class="row">
    <div class='col-md-12'>
      <div>
        <p><%= @search.name %>一覧</p>
      </div>
      <div>
        <% @searchs.each do |search| %>
          <%= link_to item_path(@search.id) do %>
          <%= image_tag search.image, size: "200x200"  %>
        <% end %>
          <%= search.name %>
          ￥<%= search.price.to_s(:delimited) %>
        <% end %>
      </div>
    </div>
  </div>
</div>