<div class = "text-left">
  <table class="border border-dark">
    <thead>
      <tr>
        <th class="border-bottom border-dark">ジャンル一覧</th>
      </tr>
    </thead>
    </tbody>
      <% genres.each do |genre| %>
        <tr>
        <td><%= link_to genre.name, admin_genre_path(genre.id) %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>